<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
    <tbody>
      <% @faqs.each do |faq| %>
      <tr>
            <td><%= faq.question %></td>
            <td><%= faq.answer %></td>
            <td><%= faq.product_id %></td>
            <td><%= link_to t(:show), faq %></td>
      <td><%= link_to t(:edit), edit_faq_path(faq) %></td>
      <td><%= link_to t(:destroy), faq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
