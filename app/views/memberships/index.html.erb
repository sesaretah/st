<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
    <tbody>
      <% @memberships.each do |membership| %>
      <tr>
            <td><%= membership.profile_id %></td>
            <td><%= membership.company_id %></td>
            <td><%= link_to t(:show), membership %></td>
      <td><%= link_to t(:edit), edit_membership_path(membership) %></td>
      <td><%= link_to t(:destroy), membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
